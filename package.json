{
  "name": "js-stdio-external-service",
  "version": "0.1.0",
  "description": "External service to use stdio with javascript",
  "main": "index.js",
  "scripts": {
    "build": "tsc && tslint features/**/*.ts src/**/*.ts",
    "test": "npm run test:tdd && npm run test:bdd",
    "test:tdd": "ava src/**/*.spec.js",
    "test:bdd": "cucumber.js --tags ~@e2e",
    "postinstall": "typings install",
    "pretest": "npm run build"
  },
  "keywords": [
    "stdio",
    "logging",
    "stdin",
    "stdout",
    "stderr",
    "console"
  ],
  "author": "Tamas Kokeny <tamas.kokeny@lab.coop>",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.15.2",
    "bottlejs": "^1.3.0",
    "chai": "^3.5.0",
    "cucumber": "^1.0.0",
    "sinon": "^1.17.4",
    "tslint": "^3.11.0",
    "typescript": "^1.9.0-dev.20160620-1.0",
    "typings": "^1.0.5"
  },
  "engines": {
    "node": ">=6.2.0"
  }
}
